<?xml version="1.0" encoding="utf-8"?>
<com.profilemaker.dpgenerator.photoprofilepic.photoeditor.views.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="@dimen/_35sdp"
    android:layout_height="@dimen/_35sdp"
    android:background="@color/BackgroundColor"
    android:layout_marginStart="@dimen/_5sdp"
    android:layout_marginEnd="@dimen/_5sdp"
    android:gravity="center">

    <com.profilemaker.dpgenerator.photoprofilepic.photoeditor.views.CornerLayout
        android:id="@+id/rfItem"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:StrokeColor="@color/itemColor"
        app:StrokeWidth="@dimen/_1sdp"
        app:iBackground="@color/itemColor"
        app:Radius="@dimen/_9sdp">

        <ImageView
            android:id="@+id/ivItem"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </com.profilemaker.dpgenerator.photoprofilepic.photoeditor.views.CornerLayout>

</com.profilemaker.dpgenerator.photoprofilepic.photoeditor.views.SquareLayout>
